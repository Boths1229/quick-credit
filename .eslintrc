{
    "root": true,
    "extends": "airbnb-base",
    "parser": "babel-eslint",
    "env": {
      "node": true,
      "es6": true,
      "mocha": true
    },
    "parserOptions": {
      "ecmaFeatures": {
        "experimentalObjectRestSpread": true
      },
      "sourceType": "module"
    },
    "plugins": [],
    "rules": {
      "linebreak-style": 0,
      "one-var": 0,
      "one-var-declaration-per-line": 0,
      "new-cap": 0,
      "consistent-return": 0,
      "no-param-reassign": 0,
      "comma-dangle": 0
    },
    "globals": {
      "document": true,
      "localStorage": false,
      "window": true
    }
  }
